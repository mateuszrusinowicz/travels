        driver.findElement(By.xpath("//*[@id=\"hotels\"]/form/div[5]/button")).click();

        WebElement noneHotels = driver.findElement(By.xpath("//*[@id=\"body-section\"]/div[5]/div[1]/div[3]/div/div/h2"));

